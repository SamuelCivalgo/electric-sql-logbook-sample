{
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[prisma]": {
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "arrowleft",
    "arrowright",
    "blurhash",
    "checkcircleo",
    "checkmark",
    "civalgo",
    "closecircleo",
    "clsx",
    "codegen",
    "datasources",
    "datetimepicker",
    "DDTHH",
    "dismissable",
    "electricsql",
    "ellipsize",
    "Entypo",
    "envsub",
    "expodoctor",
    "flatlist",
    "formatjs",
    "getcanonicallocales",
    "gorhom",
    "headlessui",
    "heroicons",
    "hookform",
    "howljs",
    "interruptible",
    "Ionicons",
    "luma",
    "mapbox",
    "morrowdigital",
    "nativewind",
    "netinfo",
    "nocheck",
    "notifee",
    "nozbe",
    "paralleldrive",
    "pluralrules",
    "pluscircle",
    "Pressable",
    "Proguard",
    "randomcolor",
    "redwoodjs",
    "sdls",
    "supabase",
    "Swipeable",
    "tailwindcss",
    "triggerer",
    "typesafe",
    "upserted",
    "uuidv",
    "watermelondb",
    "yxxx"
  ],
  "i18n-ally.pathMatcher": "{locale}/texts.json",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": [
    "../mobile/i18n",
    "mobile/i18n",
    "mobile/tools/i18n"
  ],
  "i18n-ally.annotations": false,
  "i18n-ally.translate.engines": ["google"],
  "i18n-ally.readonly": false,
  "i18n-ally.sortKeys": true,
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.enabledParsers": ["json"],
  "i18n-ally.sourceLanguage": "en"
}

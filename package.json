{
  "name": "mobile",
  "version": "1.2.3",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "current-version": "echo $npm_package_version",
    "start": "expo start --dev-client",
    "android": "rimraf android && expo run:android",
    "ios": "expo run:ios",
    "dev": "yarn start",
    "start-clean-cache": "expo start --dev-client --clear",
    "electric:generate-client": "rimraf src/generated && yarn run electric-sql generate --service https://logbook-sample-electric.fly.dev --pg-proxy-host logbook-sample-electric.fly.dev --database-name postgres --pg-proxy-port 65432 --pg-proxy-password proxyPassword --module-resolution node",
    "generate:gql": "graphql-codegen",
    "expo-doctor": "npx expo-doctor",
    "run:tsc": "tsc --noEmit",
    "postinstall": "npx sort-package-json",
    "db:migrate": "yarn prisma migrate dev --schema db/schema.prisma",
    "electric:reset-electrification": "yarn run electric:drop-all && yarn electric:fly-update && yarn electric:electrify-tables",
    "electric:drop-all": "yarn prisma db execute --file ./db/scripts/drop_electricSQL.sql - --schema ./db/scripts/script_schema.prisma",
    "electric:electrify-tables": "yarn prisma db execute --file ./db/scripts/electrify_tables.sql - --schema ./db/scripts/script_schema.prisma",
    "electric:fly-new-launch": "fly launch --copy-config --ha=false",
    "electric:fly-update": "fly deploy"
  },
